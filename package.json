  "repository": {
    "type": "git",
    "url": "https://github.com/chadlatham/ng2-webpack-heroku"
  },
  "scripts": {
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "heroku-postbuild": "knex migrate:latest; rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "knex": "knex",
    "nf": "nf start",
    "postinstall": "typings install",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "typings": "typings"
  },
